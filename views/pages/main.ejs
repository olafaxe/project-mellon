<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="stylesheet" href="grid.css" />
    <link
      href="https://fonts.googleapis.com/css?family=Press+Start+2P&display=swap"
      rel="stylesheet"
    />

    <title>MAIN</title>
  </head>
  <body>
    <div class="main-container">
      <div class="ola-container"><img class="main-ola" src="ola.png" /></div>
      <% if(typeof username != "undefined") {%>
      <div class="main-content">
        <h1>LOGGED IN as</h1>
        <h2>User: <%=username%></h2>
        <%}%>
        <h4>Other cool peeps:</h4>
        <%users.forEach(e => {if(e.user != username){%>
        <p><%=e.user%></p>
        <%}});%>
      </div>
      <div class="logout-cont">
        <a class="logout-btn login-button" href="/bye">Logout</a>
      </div>
    </div>
  </body>
</html>
